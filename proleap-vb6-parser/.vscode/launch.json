    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "TestGenerator",
            "request": "launch",
            "mainClass": "io.proleap.vb6.TestGenerator",
            "projectName": "proleap-vb6-parser"
        },
        {
            
            "name": "Debug ANTLR4 grammar",
            "type": "antlr-debug",
            "request": "launch",
            "input": "tet.bas",
            "grammar": "src/main/antlr4/io/proleap/vb6/VbaParser.g4",
            "printParseTree": true,
            "visualParseTree": true
        }
    ]
}
    